package org.java.variableEx;

public class CastingEX {
	public static void main(String[] args) {
		System.out.println("형 변화");
		System.out.println("자동형변환");
		
		int i = 10;
		double d = 1.1;
		double d2 = i + d;
		double d3 = 20 + 1.5;
		
		byte b1 = 20;
		byte b2 = 30;
				
		short s1 = 10;
		int i3 = b1 + b2; //byte+byte=int
	}

}
