package org.study.innerclass;

interface In1{
	void m1();
}

abstract class AbS1{
	abstract void m1();
}

class C1{
	void m1() {
		System.out.println("c1");
	}
}

public class AnonymousClassEX {
	public static void main(String[] args) {
		new In1() {
			@Override
			public void m1() {
				System.out.println("m1");
			}
		};
		
		new AbS1() {
			@Override
			void m1() {
				System.out.println("추상클래스 m2");
			}
		};
		
		new C1() {
			@Override
			void m1() {
				System.out.println("일반클래스");
				super.m1();
			}
		};
	}
}
