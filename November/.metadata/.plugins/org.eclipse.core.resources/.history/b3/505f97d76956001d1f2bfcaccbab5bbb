package org.java.variableEx;

public class VrEx01 {
	
	public static void main(String[] args) {
		// 1. 변수
		/*
		 1. 변수를 선언
		 2. 변수를 초기화 -> data에 할당
		 3. 변수를 메모리에 할당 
		 */		
		 //Java의 기본 타입 primitive types		 		 
		
		//정수형
		byte b;// byte형의 변수 b를 선언함
		b = 10;// 변수  b를 정수형 data 10으로 초기화 -> 메모리에 할당
		 
	}

}
