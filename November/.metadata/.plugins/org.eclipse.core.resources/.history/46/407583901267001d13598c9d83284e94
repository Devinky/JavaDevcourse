package org.study.innerclass;

interface I1{
	void i1();
}

class A implements I1{
	@Override
	public void i1() {
		System.out.println("A i1 오버라이딩");
	}
}

interface I2 extends I1{
	abstract void i2();
}

interface I3 extends I2{
	abstract void i3();
}

public class AnonymousClassEX02 {
	public static void main(String[] args) {
		
		A a = new A();
		I1 i11 = new A();
		i11.i1();
		
		I1 i2 = new I1() {
			@Override
			public void i1() {
				System.out.println("i1오버라이딩");
			}
		};
		i2.i1();
	}
}
