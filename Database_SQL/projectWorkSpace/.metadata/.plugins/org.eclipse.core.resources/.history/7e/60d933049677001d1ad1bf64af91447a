package test;

public class Test1 {
	
	public int solution(int num1, int num2) {
        int answer = 0;
        
        answer = num1/num2;
            
        return answer;
    }
	
	public static void main(String[] args) {
		
		Test1 test = new Test1();
		int result = test.solution(40, 4);
		
		System.out.print(result);
		
	}

}
